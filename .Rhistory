triangulate_matrix <- function(mat, direction = c('l', 'u'), k=0) {
if (direction == 'l') {
return( as.matrix(Matrix::tril(mat, k = k)) )
} else if (direction == 'u') {
return( as.matrix(Matrix::triu(mat, k = k)) )
} else {
message('Error: not valid direction.')
return( NULL )
}
}
